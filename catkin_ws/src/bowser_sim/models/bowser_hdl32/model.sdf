<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="bowser_hdl32">
    <gravity>false</gravity>
    <link name="hdl32_base">
      <!-- <gravity>false</gravity>     -->
      <pose>0 0 0.029335 0 0 0</pose>
      <inertial>
        <mass>1.2</mass>
        <inertia>
          <ixx>0.001087473</ixx>
          <iyy>0.001087473</iyy>
          <izz>0.001092437</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="base_collision">
        <geometry>
          <cylinder>
            <radius>.04267</radius>
            <length>.05867</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="base_visual">
        <pose>0 0 -0.029335 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://bowser_hdl32/meshes/velodyne_base.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    
    <link name="hdl32_top">
      <pose>0 0 0.095455 0 0 0</pose>
      <!-- <gravity>false</gravity> -->
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.000090623</ixx>
          <iyy>0.000090623</iyy>
          <izz>0.000091036</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="top_collision">
        <geometry>
          <cylinder>
            <radius>0.04267</radius>
            <length>0.07357</length>
          </cylinder>
        </geometry>
      </collision>
   <!--    <visual name="top_visual">
        <pose>0 0 -0.0376785 0 0 1.5707</pose>
        <geometry>
          <mesh>
            <uri>model://bowser_hdl32/meshes/velodyne_top.dae</uri>
          </mesh>
        </geometry>
      </visual> -->

      <sensor type="ray" name="sensor">
        <pose>0 0 -0.004645 0 0 0</pose>
        <visualize>false</visualize>
        <update_rate>30</update_rate>
        <ray>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.1</stddev>
          </noise>

          <scan>
            <vertical>
              <samples>32</samples>
              <resolution>1</resolution>
              <min_angle>-3.14159</min_angle>
              <max_angle>3.14159</max_angle>
            </vertical>
            <horizontal>
                <samples>256</samples>
                <resolution>1</resolution>
                <!-- <min_angle>-.2617993878</min_angle>
                <max_angle>.2617993878</max_angle> -->
                <min_angle>-3.14159</min_angle>
                <max_angle>3.14159</max_angle>
            </horizontal>
          </scan>

          <range>
            <min>0.1</min>
            <max>10</max>
            <resolution>0.02</resolution>
          </range>
        </ray>
        <plugin name="laser" filename="libgazebo_ros_velodyne_laser.so">
            <robotNamespace></robotNamespace>
            <topicName>/cloud_in</topicName>
	        	<!-- <topicName>/bowser/velodyne</topicName> -->
		        <!-- <frameName>velodyne_link</frameName> -->
            <frameName>bowser2/bowser_hdl32/hdl32_top</frameName>
	      </plugin>
      </sensor>
    </link>

    <joint type="revolute" name="joint">
      <pose>0 0 -0.036785 0 0 0</pose>
      <parent>hdl32_base</parent>
      <child>hdl32_top</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-10000000000000000</lower>
          <upper>10000000000000000</upper>
        </limit>
      </axis>
    </joint>

<!--     <plugin name="velodyne_control" filename="libvelodyne_plugin.so">
      <velocity>0</velocity>
    </plugin> -->


  </model>
</sdf>
